Ext2 = Off
Ext3 = Off
Ext4 = Off
Ext5 = Off
Ext6 = Off
Ext7 = Off
Ext8 = Off
IO1 = 0
IO2 = 0
IO3 = 0

[AWG]
Mode = AWG
Clock = 10 GHz
Output1 = On
Output2 = On
